<script>
    (function ($) {
        $.fn.NumberFix = function (options) {
            var configs = {
                addEle: '.add',
                addEvent: function () { },
                subEle: '.sub',
                subEvent: function () { },
                showEle: '.center',
                format: null,
                changeVal: 1 //每次点击增加的量
            };

            if (options) {
                $.extend(configs, options);
            }

            $(this).each(function () {
                var add = $.type(configs.addEle) === 'string' ? $(this).find(configs.addEle) : configs.addEle;
                var sub = $.type(configs.subEle) === 'string' ? $(this).find(configs.subEle) : configs.subEle;
                var show = $.type(configs.showEle) === 'string' ? $(this).find(configs.showEle) : configs.showEle;

                add.on('click', function () {
                    var i = getval(show);
                    i = i + (configs.changeVal || 1);
                    if (i > configs.max) i = configs.max;
                    setval(show, i);
                    configs.addEvent.call(this, i, show, add, sub);
                });

                sub.on('click', function () {
                    var i = getval(show);
                    i = i - (configs.changeVal || 1);
                    if (i < configs.min) i = configs.min;
                    setval(show, i);
                    configs.subEvent.call(this, i, show, add, sub);
                });
            });

            function getval(obj) {
                return parseFloat(obj.is(':text') ? obj.val() : obj.text()) || 0;
            }

            function setval(obj, v) {
                v = $.type(configs.format) === 'function' ? configs.format.call(this, parseFloat(v)) : v;
                $("#num").val(v);
                obj.is(':text') ? (obj.val(v)) : (obj.text(v));
            }
        };
    })(jQuery);

    $(function () {
        var cucun = 1; //库存量
        $('.pay_footer').NumberFix({
            addEvent: function (crt, show, addbtn, subbtn) {
                subbtn.removeClass('btndis'); //恢复按钮
            },
            subEvent: function (crt, show, addbtn, subbtn) {
                if (crt<=cucun) {
                    show.text(cucun);//重置为存为
                    subbtn.addClass('btndis');
                }
            }
        });
    });
    
    
    function payOrder(){
    	window.location.href="${rc.contextPath}/weixin/wxconfirmOrderList.htm?standId=$!standardOrder.standardorderId&offerPrice=$!standardOrder.offerPrice&num="+$("#num").val();
    }
